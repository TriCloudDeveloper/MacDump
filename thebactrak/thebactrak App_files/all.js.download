function js_height_init() {

    (function($) {

        $(".js-height-full").not(".js-height-full-check").height($(window).height());
        $(".js-height-parent").each(function() {
            $(this).height($(this).parent().first().height());
        });

        if ($(window).width() > 600) {
            $(".js-height-full.js-height-full-check").height($(window).height());
        }
    })(jQuery);
}




